            @Html.DisplayFor(modelItem => item.EngineId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeCreate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeImplement)
